{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://server.cehospitalitygroup.com/api/$1"
    },
    {
      "src": "(.*)",
      "dest": "/index.html"
    }
  ],
  "public": true
}